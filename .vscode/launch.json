    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: PostEdit Main (Full Benchmark)",
            "type": "debugpy",
            "request": "launch",
            "program": "./main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--path_to_prompts", "benchmarks/instructions/editing_pie_bench_700.csv",
                "--path_to_images", "benchmarks/images/pie_bench_700_images"
            ]
        },
        {
            "name": "Python: PostEdit (Single Image Editing)",
            "type": "debugpy",
            "request": "launch",
            "program": "./main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--single_image",
                "--path_to_prompts", "benchmarks/instructions/editing_pie_bench_700.csv",
                "--path_to_images", "benchmarks/test"
            ]
        }
    ]
}